# Firebase Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# Web Configuration
FIREBASE_WEB_API_KEY=your_web_api_key_here
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_WEB_APP_ID=your_web_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id

# Android Configuration
FIREBASE_ANDROID_API_KEY=your_android_api_key
FIREBASE_ANDROID_APP_ID=your_android_app_id

# iOS Configuration
FIREBASE_IOS_API_KEY=your_ios_api_key
FIREBASE_IOS_APP_ID=your_ios_app_id
FIREBASE_IOS_BUNDLE_ID=com.example.adminApp

# macOS Configuration
FIREBASE_MACOS_API_KEY=your_macos_api_key
FIREBASE_MACOS_APP_ID=your_macos_app_id
FIREBASE_MACOS_BUNDLE_ID=com.example.adminApp
